public class MethodExThree { 
	int var; // 멤버 필드
	// 생성자
	// 메서드
	public void setVar(int var) {
		this.var = var;
	}
	public int getVar() {
		return var;
	}
	public static void main(String[] args) {
		MethodExThree me = new MethodExThree();
		me.setVar(1000);
		System.out.println("var : " + me.getVar());
	}
}

